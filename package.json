{
  "name": "@tosee/misc",
  "version": "2.3.7",
  "description": "Framework based on Koa and Typescript",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "scripts": {
    "coverage": "nyc --reporter=lcov --reporter=html npm test&&nyc report",
    "test": "cross-env NODE_ENV=test alsatian ./test/__tests__/*.spec.ts",
    "start": "cross-env NODE_ENV=test node test/index.js",
    "version": "rm -rf ./dist/*&&tsc&&typedoc --out ./docs ./lib",
    "codecov": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fullstackoverflow/Misc"
  },
  "author": "tosee",
  "license": "MIT",
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "@tosee/log": "^1.0.2",
    "@types/async": "^2.4.1",
    "@types/glob": "^7.1.1",
    "@types/joi": "^14.3.2",
    "@types/keygrip": "^1.0.1",
    "@types/koa": "^2.0.39",
    "@types/koa-router": "^7.0.35",
    "@types/koa-session": "^5.7.4",
    "@types/koa__cors": "^2.2.3",
    "@types/node": "^11.10.5",
    "@types/node-schedule": "^1.2.2",
    "@types/read-pkg-up": "^3.0.1",
    "async": "^2.6.2",
    "chalk": "^2.3.2",
    "chokidar": "^2.0.4",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "glob": "^7.1.2",
    "koa": "^2.6.2",
    "koa-body": "^4.0.6",
    "koa-router": "^7.4.0",
    "koa-session": "^5.10.1",
    "moment": "^2.23.0",
    "node-schedule": "^1.3.1",
    "read-pkg-up": "^4.0.0",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@types/supertest": "^2.0.7",
    "alsatian": "^2.4.0",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "nyc": "^14.1.1",
    "supertest": "^3.3.0",
    "tap": "^13.1.9",
    "tap-nyc": "^1.0.3",
    "ts-node": "^8.0.3",
    "typescript": "^3.2.2"
  }
}
